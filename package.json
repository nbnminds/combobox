{
  "name": "@nbnminds/combobox",
  "version": "0.0.2",
  "description": "A powerful Svelte 5 dropdown component with single/multi-select and AJAX autocomplete",
  "license": "MIT",
  "author": "NBN Minds Private Limited",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nbnminds/combobox.git"
  },
  "homepage": "https://github.com/nbnminds/combobox#readme",
  "bugs": {
    "url": "https://github.com/nbnminds/combobox/issues"
  },
  "keywords": [
    "svelte",
    "svelte5",
    "dropdown",
    "select",
    "multiselect",
    "autocomplete",
    "component",
    "combobox"
  ],
  "type": "module",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint ."
  },
  "dependencies": {
    "clsx": "^2.1.0"
  },
  "peerDependencies": {
    "@lucide/svelte": ">=0.400.0",
    "bits-ui": "^2.0.0",
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/js": "^9.39.1",
    "@lucide/svelte": "^0.561.0",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.48.5",
    "@sveltejs/package": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^24",
    "autoprefixer": "^10.4.20",
    "bits-ui": "^2.14.4",
    "clsx": "^2.1.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.13.0",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "publint": "^0.3.15",
    "svelte": "^5.43.8",
    "svelte-check": "^4.3.4",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.2"
  }
}
